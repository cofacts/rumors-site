{
  "name": "rumors-site",
  "version": "0.0.1",
  "description": "Rumors list / creation UI, with server-side rendering",
  "main": "index.js",
  "author": "MrOrz",
  "license": "MIT",
  "scripts": {
    "dev": "babel-node server",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest .",
    "build": "docker build -t mrorz/rumors-site:latest .",
    "build:next": "next build",
    "build:server": "babel server.js -o server.dist.js",
    "init:autotrack": "autotrack -o static/$npm_package_config_autotrackFileName -p eventTracker,outboundLinkTracker,urlChangeTracker",
    "start": "NODE_ENV=production node server.dist.js"
  },
  "config": {
    "autotrackFileName": "autotrack.custom.js"
  },
  "dependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-assets": "^0.2.0",
    "babel-plugin-transform-css-import-to-string": "^0.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.5",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.4.1",
    "next": "^4.1.4",
    "next-routes": "^1.1.0",
    "normalize.css": "^7.0.0",
    "nprogress": "^0.2.0",
    "postcss-color-function": "^4.0.1",
    "postcss-css-variables": "^0.8.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-radio-group": "^3.0.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-duck": "^1.0.2",
    "redux-thunk": "^2.2.0",
    "string-similarity": "^1.2.0",
    "styled-jsx": "^2.1.3"
  },
  "devDependencies": {
    "autotrack": "^2.4.1",
    "babel-eslint": "^8.0.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-import-resolver-babel-module": "^4.0.0-beta.3",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "prettier": "^1.8.2",
    "redux-devtools-extension": "^2.13.2"
  }
}
